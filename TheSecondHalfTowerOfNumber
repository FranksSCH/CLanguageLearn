#include <stdio.h>
#include <stdlib.h>

int main() {
    int a, b, i;
    a = 1;
    scanf("%d", &i);
    while (a <= i) {
        b = 1;
        while (b <= a) {
            printf("*");
            b = b + 1;
        }
        printf("\n");
        a = a + 1;
    }
    system("pause");
    return 0;
}
